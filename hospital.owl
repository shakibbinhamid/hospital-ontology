<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/shakib/ontologies/hospital#"
     xml:base="http://www.semanticweb.org/shakib/ontologies/hospital"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:hospital="http://www.semanticweb.org/shakib/ontologies/hospital#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital">
        <owl:versionInfo>0.0.1</owl:versionInfo>
        <rdfs:comment xml:lang="en">A simple ontology for hospitals. Made by Shakib-Bin Hamid, ID: 25250094, EMAIL: sh3g12@soton.ac.uk</rdfs:comment>
        <rdfs:label xml:lang="en">Hospital Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#basedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#basedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Staff"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Building"/>
        <rdfs:comment xml:lang="en">&lt;staff&gt; basedIn &lt;building&gt;. A member of staff can only be based in ONE building (functional property).</rdfs:comment>
        <rdfs:label xml:lang="en">Based in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#bedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#bedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#hasBed"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Bed"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Ward"/>
        <rdfs:comment xml:lang="en">&lt;bed&gt; bedIn &lt;ward&gt;. A bed can only be in ONE ward (functional property). Inverse of hasBed.</rdfs:comment>
        <rdfs:label xml:lang="en">Bed in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#carriedOutBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#carriedOutBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#MedicalProcedure"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Staff"/>
        <rdfs:comment xml:lang="en">&lt;medical procedure&gt; carriedOutBy &lt;staff&gt;. e.g. Consultation_a carriedOutBy Mr._Jones, where Consultation_a is a MedicalProcedure and Mr._Jones is a Staff. A Staff can carry out multiple medical procedures, and a medical procedure can be carried out by multiple staff.</rdfs:comment>
        <rdfs:label xml:lang="en">Carried out by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#carriedOutOn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#carriedOutOn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#hasProcedure"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#MedicalProcedure"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Patient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#controlsUnit -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#controlsUnit">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Department"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Unit"/>
        <rdfs:comment xml:lang="en">&lt;department&gt; controlsUnit &lt;unit&gt;. A unit can be controlled by only ONE department, but a department may have multiple units under its control (inverse functional property).</rdfs:comment>
        <rdfs:label xml:lang="en">Controls Unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#hasBed -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#hasBed">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Ward"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Bed"/>
        <rdfs:comment xml:lang="en">&lt;ward&gt; hasBed &lt;bed&gt;. A ward can have any number of beds, but a bed can only be in one ward (inverse functional property). Inverse of bedIn.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Bed</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#hasOperation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#hasOperation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#hasProcedure"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#operatedOn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Patient"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Operation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#hasProcedure -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#hasProcedure">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Patient"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#MedicalProcedure"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#occupiedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#occupiedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Bed"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#In-Patient"/>
        <rdfs:comment xml:lang="en">&lt;bed&gt; occupiedBy &lt;in patient&gt;. Only In-Patients can occupy beds and only one bed. Similarly a bed can only be occupied by one In-Patient (inverse functional and functional).</rdfs:comment>
        <rdfs:label xml:lang="en">Occupied By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#operatedOn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#operatedOn">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#carriedOutOn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Operation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Patient"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#requiresPhysician -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#requiresPhysician">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#MedicalProcedure"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Physician"/>
        <rdfs:comment xml:lang="en">&lt;medical procedure&gt; requiresPhysician &lt;physician&gt;. A MedicalProcedure can require any number of Physicians and a Physician may be required by any number of MedicalProcedures.</rdfs:comment>
        <rdfs:label xml:lang="en">Requires Physican</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#usesWard -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#usesWard">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Unit"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Ward"/>
        <rdfs:comment xml:lang="en">&lt;unit&gt; usesWard &lt;ward&gt;. A Unit may use any number of wards, and a ward may be used by any number of units.</rdfs:comment>
        <rdfs:label xml:lang="en">Uses Ward</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#worksIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#worksIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Staff"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Department"/>
        <rdfs:comment xml:lang="en">&lt;staff&gt; worksIn &lt;department&gt;. A staff can only work in ONE department (functional property). A department can have any number of employees.</rdfs:comment>
        <rdfs:label xml:lang="en">Works in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#performedOn -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#performedOn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#MedicalProcedure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>&lt;medical procedure&gt; performedOn &lt;datetime&gt;. Medical procedures are performed on exactly one day (functional property).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Administrator -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Administrator">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Staff"/>
        <rdfs:comment xml:lang="en">Administrators are staff.</rdfs:comment>
        <rdfs:label xml:lang="en">Administrator</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Anaesthetist -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Anaesthetist">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Physician"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Radiologist"/>
        <rdfs:comment xml:lang="en">Anaesthetists specialise in Anaesthesia.</rdfs:comment>
        <rdfs:label xml:lang="en">Anaesthetist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Anesthesia -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Anesthesia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#MedicalSpeciality"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Radiology"/>
        <rdfs:label xml:lang="en">Anaesthesia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Bed -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Bed">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#HospitalComponent"/>
        <rdfs:comment xml:lang="en">Beds are placed in wards in a part-whole relationship, hasBed or bedIn</rdfs:comment>
        <rdfs:label xml:lang="en">Bed</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Building -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Building">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#HospitalComponent"/>
        <rdfs:comment xml:lang="en">Buildings have staff working in them by the relation basedIn. A building is not assigned to any particular ward, room, department or unit.</rdfs:comment>
        <rdfs:label xml:lang="en">Building</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#CardiologyDepartment -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#CardiologyDepartment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Department"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#CardiologyPatient -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#CardiologyPatient">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Patient"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#hasProcedure"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#carriedOutBy"/>
                                <owl:someValuesFrom>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#worksIn"/>
                                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#CardiologyDepartment"/>
                                    </owl:Restriction>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">Patient treated by a staff who works in the cardiology department.</rdfs:comment>
        <rdfs:label xml:lang="en">Cardiology Patient</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Consultation -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Consultation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#MedicalProcedure"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Operation"/>
        <rdfs:comment xml:lang="en">A consultation is a medical procedure carried out by a staff on a patient.</rdfs:comment>
        <rdfs:label xml:lang="en">Consultation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#ConsultingRoom -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#ConsultingRoom">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Room"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#OperatingTheatre"/>
        <rdfs:comment xml:lang="en">A Consulting room is used for a Consultation.</rdfs:comment>
        <rdfs:label xml:lang="en">Consulting Room</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Department -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Department">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#HospitalComponent"/>
        <rdfs:comment xml:lang="en">A department can be thought of an administrative faction of a hospital. Staffs work in departments by the worksIn relation. Departments are in charge of specialist units by the controlsUnit relation. They do not control wards directly since that is a function performed by functional factions like units.</rdfs:comment>
        <rdfs:label xml:lang="en">Department</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#EmergencyCareDepartment -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#EmergencyCareDepartment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Department"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#FemaleWard -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#FemaleWard">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Ward"/>
        <rdfs:comment xml:lang="en">A ward for Female patients.</rdfs:comment>
        <rdfs:label xml:lang="en">Female Ward</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Hospital -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Hospital"/>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#HospitalComponent -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#HospitalComponent">
        <rdfs:comment xml:lang="en">A hospital component is everything that is in a hospital&apos;s structural and adminstrative entity. So, it includes wards, beds, departments, etc. but does not have people, medical procedures, specialities etc.</rdfs:comment>
        <rdfs:label xml:lang="en">Hospital Component</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#In-Patient -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#In-Patient">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Patient"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Out-Patient"/>
        <rdfs:comment xml:lang="en">Patients that are admitted to the hospital and occupies a bed.</rdfs:comment>
        <rdfs:label xml:lang="en">In-Patient</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#MaleWard -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#MaleWard">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Ward"/>
        <rdfs:comment xml:lang="en">A ward for male patients.</rdfs:comment>
        <rdfs:label xml:lang="en">Male Ward</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#MedicalProcedure -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#MedicalProcedure">
        <rdfs:comment xml:lang="en">A medical procedure is carried out by a Staff and carried out on a Patient on a Datetime.</rdfs:comment>
        <rdfs:label xml:lang="en">Medical Procedure</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#MedicalSpeciality -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#MedicalSpeciality">
        <rdfs:comment xml:lang="en">A Physician can specialise in medical specialities.</rdfs:comment>
        <rdfs:label xml:lang="en">Medical Speciality</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#MixedGenderWard -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#MixedGenderWard">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#FemaleWard"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#MaleWard"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Ward"/>
        <rdfs:comment xml:lang="en">A ward for both male and female patients.</rdfs:comment>
        <rdfs:label xml:lang="en">Mixed Gender Ward</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Nurse -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Nurse">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Staff"/>
        <rdfs:comment xml:lang="en">Nurses are staff.</rdfs:comment>
        <rdfs:label xml:lang="en">Nurse</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#Office -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#Office">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Room"/>
        <rdfs:comment xml:lang="en">An office is used for administrative purposes.</rdfs:comment>
        <rdfs:label xml:lang="en">Office</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shakib/ontologies/hospital#OperatingTheatre -->

    <owl:Class rdf:about="http://www.semanticweb.org/shakib/ontologies/hospital#OperatingTheatre">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shakib/ontologies/hospital#Room"/>
        <rdfs:comment xml:lang="en">A Operating Theatre is used for Operation.</rdfs:comment>
        <rdfs:label xml:lang="en">Operating Theatre</rdfs:label>
